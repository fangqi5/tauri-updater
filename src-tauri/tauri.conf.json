{
    "build": {
        "beforeDevCommand": "yarn dev",
        "beforeBuildCommand": "yarn build",
        "devPath": "http://localhost:1420",
        "distDir": "../dist"
    },
    "package": {
        "productName": "tauri-updater",
        "version": "0.0.1"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "shell": {
                "all": false,
                "open": true
            }
        },
        "windows": [
            {
                "title": "tauri-updater",
                "width": 800,
                "height": 600
            }
        ],
        "security": {
            "csp": null
        },
        "bundle": {
            "resources": ["node"],
            "active": true,
            "targets": "all",
            "identifier": "com.fangqi5",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ]
        },
        "updater": {
            "active": true,
            "dialog": true,
            "endpoints": [
                "http://localhost:3000/checkUpdate"
            ],
            "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDVFMDE2MzVGNTZENjEyQ0IKUldUTEV0WldYMk1CWHB1L05scXpGRkM1VWpFeERBa0JidWNoQytZV1BrZTkxM2NnbkRPaE9NSnAK",
            "windows": {
                "installMode": "passive",
                "installerArgs": []
            }
        }
    }
}
